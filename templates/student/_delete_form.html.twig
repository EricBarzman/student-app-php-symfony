<form method="post" action="{{ path('app_student_delete', {'id': student.id}) }}" onsubmit="return confirm('Are you sure you want to delete this file?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ student.id) }}">
    <div class="col-5 mt-3 container">
        <div class="d-flex flex-column justify-content-around">
            <button type="submit" class="btn btn-danger m-3">Delete student</button>
        </div>
    </div>
</form>
